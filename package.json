{
  "name": "@nspencer4/monorepo-poc",
  "version": "0.1.1",
  "license": "MIT",
  "pre-commit": [
    "pre-commit"
  ],
  "scripts": {
    "audit.all.deps": "lerna run audit.deps",
    "audit.all.unused": "lerna run audit.unused.deps",
    "audit.changed.deps": "lerna run audit.deps --since HEAD~1",
    "audit.changed.unused": "lerna run audit.unused --since HEAD~1",
    "build.all": "lerna run build",
    "build.changed": "lerna run build --since HEAD~1",
    "install.all.deps": "yarn install && lerna exec -- yarn install",
    "install.changed.deps.ci": "lerna exec -- yarn install --since HEAD~1",
    "lint.all": "lerna run lint",
    "lint.changed": "lerna run lint --since HEAD~1",
    "postinstall": "lerna bootstrap",
    "publish": "lerna publish from-git",
    "test.all": "lerna run test",
    "test.changed": "lerna run test --since HEAD~1",
    "storybook.start": "start-storybook -p 5555 --no-manager-cache",
    "storybook.build": "build-storybook --debug-webpack",
    "version": "lerna version --conventional-commits",
    "prepare": "npx husky install",
    "check-pipeline-readiness": "yarn run audit.changed.unused && npm run audit.changed.deps && npm run lint.changed && npm run test.changed && npm run build.changed"
  },
  "dependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@storybook/addon-a11y": "^6.3.8",
    "@storybook/addon-jest": "^6.3.8",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.3.8",
    "@types/jest": "^27.0.1",
    "@types/react": "17.0.21",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "bootstrap": "^5.1.1",
    "css-loader": "5.2.6",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.4.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.25.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.2",
    "jest": "^27.2.0",
    "lerna": "^4.0.0",
    "prettier": "^2.4.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "sass": "^1.42.1",
    "sass-loader": "10.1.1",
    "storybook-addon-themes": "^6.1.0",
    "style-loader": "2.0.0",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
  },
  "peerDependencies": {
    "typescript": "^4.4.3"
  }
}
